#include <iostream>
#include <stdlib.h>
using namespace std;
int a=0;
char c[10][10]={" "," ","abc","def","ghi","jkl","mno","pqrs","tuv","wxyz"};
int total[10]={0,0,3,3,3,3,3,4,3,4};

void combine(int number[],int position[],int index,int n)
{
    if(index==n)
    {
	  for(int i=0;i<n;i++)
	  {
		a++;
		printf("%c",c[number[i]][position[i]]);
		if(a%n==0)
		    printf("\n");
	  }
	  return ;
    }
    for (position[index] = 0;position[index]<total[number[index]];position[index]++)
	  combine(number,position,index+1,n);
}

int main()
{
    int number[11];
    int n,i;
    int position[11]={0};	
    printf("请输入电话号码的长度:\n");
    scanf("%d",&n);	
    printf("请输入一个电话号码:\n");
    for (i=0;i<n;i++)
	  scanf("%d",&number[i]);
    combine(number,position,0,n);
    printf("%d",a/n);
    return 0;
}
