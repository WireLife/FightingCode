
#include <iostream>
using namespace std;
 
int main(void)
{
	int i,j,k,m,c=0;
	for ( i=2;i!=6;++i)
		for(j=2;j!=6;++j)
			for( k=2;k!=6;++k)
				for(m=2;m!=6;++m) 
					if(i!=j&&i!=k&&j!=k&&i!=m&&j!=m&&k!=m&&
						i<j&&k<m&&i<m)
	               {
					   cout<<"1 "<<i<<" "<<j<<endl;
					   cout<<k<<" "<<m<<" "<<"6"<<endl;
					   cout<<endl;
					   c++;
	               }
	
	cout<<c;
	return 0;
}
