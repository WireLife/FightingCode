#include <cstdio>
#include <iostream>
using namespace std;

int fun(int m,int n)
{
    if(m==0||n==1)
        return 1;
    if(m <n)
        return fun(m,m);
    else
        return fun(m,n-1)+fun(m-n,n);
}
int main()
{
    int n,m,t;
    cin>>t;
    while(t--)
    {
        cin>>m>>n;
        cout<<fun(m,n)<<endl;
    }
    return 0;
}
