#include<iostream>
using namespace std;

void check(char *s)
{
	int n=0, sum[11], i,j=0;
	for (i = 0; i <12; i++)
	{
		if (s[i] != '-') 
		{
			sum[j] = (s[i] - '0') * (j + 1);
			n += sum[j];
			j++;
		}
	}
	if (n % 11 == (s[12]-'0'))
		cout << 1 << endl;
	else
		cout << 0 << endl;
}

int main()
{
	char s[13];
	cout << "请输入ISBN码：\n" << endl;
	for (int i = 0; i < 13; i++)
	{
		cin >> s[i];
	}
	check(s);
	system("pause");
	return 0;
}
