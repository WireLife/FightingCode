}
void f(char a[7],char b[7])
{
    int i,sr1=0,sb1=0,sg1=0,sr2=0,sb2=0,sg2=0;
    for(i=0;i<7;i++)
        if(a[i]=='r')
            sr1++;
        else if(a[i]=='g')
            sg1++;
        else if(a[i]=='b')
            sb1++;
    for(i=0;i<7;i++)
        if(b[i]=='r')
            sr2++;
        else if(b[i]=='g')
            sg2++;
        else if(b[i]=='b')
            sb2++;
    if(sr1==sr2&&sb1==sb2&&sg1==sg2)
        g(a,b);
    else 
        printf("FALSE\n");
}
