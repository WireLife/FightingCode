#include<stdio.h>
int k;
int f(int m,int n)
{
    int i;
    for(i=m>n?m:n;;i++){
        if(i%m==0&&i%n==0)
            return i;
    }
}
void g(int m,int n)
{
    int i;
    for(i=m>n?n:m;;i--){
        if(i==1)
            break;
        else if(m%i==0&&n%i==0){
            m/=i;
            n/=i;
        }
    }
    if(n==1)
        printf("%d",m);
    else 
        printf("%d/%d",m,n);
}
int son(char* p)
{
    int i,num=0,n;
    while(*p!='/'){
        switch(*p){
            case '0':n=0;break;
            case '1':n=1;break;
            case '2':n=2;break;
            case '3':n=3;break;
            case '4':n=4;break;
            case '5':n=5;break;
            case '6':n=6;break;
            case '7':n=7;break;
            case '8':n=8;break;
            case '9':n=9;break;
        }
        num=10*num+n;
    }
    return num;
}
int mother(char* p)
{
    int num=0,n=0;
    while(1){
        if(*p=='/'){
            while(*p!='\0'){
                switch(*p){
                    case '0':n=0;break;
                    case '1':n=1;break;
                    case '2':n=2;break;
                    case '3':n=3;break;
                    case '4':n=4;break;
                    case '5':n=5;break;
                    case '6':n=6;break;
                    case '7':n=7;break;
                    case '8':n=8;break;
                    case '9':n=9;break;
                }
                num=10*num+n;
                p++;
            }
            return num;
        }
        p++;
    }
    return num;
}
int main()
{
    int i,m[10]={0},s[10]={0},n,ss;
    char a[10][6]={0};
    scanf("%d",&k);
    for(i=0;i<k;i++)
        scanf("%s",a[i]);
    for(i=0;i<k;i++)
        m[i]=mother(a[i]);
    for(i=0;i<k;i++)
        s[i]=son(a[i]);
    n=f(m[0],m[1]);
    for(i=2;i<k;i++)
        n=f(n,m[i]);
    for(i=0;i<k;i++){
        s[i]=s[i]*n/m[i];
        ss+=s[i];
    }
    g(ss,n);
    return 0;
}
