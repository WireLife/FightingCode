#include<iostream>   
#include<string> 
#include<cstdio>
using namespace std;
int main() {
	int a, b, c;
	scanf("%ld/%ld", &a, &b);
	//cout << "输出结果为：";
	while (true){ 
		if (b%a)
			c = b / a + 1;
		else{
			c = b / a;
			a = 1;
		}
		if (a == 1){
			cout << "1/" << c;
			break;
		}
		else
			cout << "1/" << c << "+";
		a = a*c - b; 
		b = b*c; 
		if (a == 3){ 
			cout << "1/" << b / 2 << "+" << "1/" << b;
			break;
		}
	}
 
 
 
	cout << endl;
	system("pause");
	
}
