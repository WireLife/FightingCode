#include <stdio.h>
#include <math.h>
#define MAXN  	400
 
int a[MAXN], b[MAXN];
int main(void)
{
	int i, j;
	int flag, n=0;
	int sum, num = 0;
	for (i=2; i<=1993; ++i) {
		flag = 1;
		for (j=2; j<=sqrt(i); ++j) {
			if (i % j == 0) {
				flag = 0;
				break;
			}
		}
		if (flag) {
			printf("%d ", i);
			a[n++] = i;
		}
	}
	putchar('\n');
	for (i=1; i<n; ++i) {
		b[i-1] = a[i] - a[i-1];
	}
	for (i=0; i<n-1; ++i)
		printf("%d ", b[i]);
	
	for (i=0; i<n-1; ++i) {
		sum = 0;
		for (j=i; j < n-1 && sum < 1898; ++j) {
			sum += b[j];
		}
		if (sum == 1898)
			++num;
	}
	printf("\n共有%d种\n", num);
	return 0;
}
