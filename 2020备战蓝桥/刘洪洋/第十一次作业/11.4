#include<iostream>
#include <cstdio>
using namespace std;
int main() {
	string a;
	getline(cin,a);
	if (sizeof(a)>=256)
	cout<<"The number is too many"<<endl; 
	int k = 0;
	for (int i = 0; i<sizeof(a); i++) {
		if (a[i]>='0' && a[i]<='9')
		k++;
	}
	cout <<"Peking University is set up at "<< k << endl;
	return 0;
}
