#include <iostream>
#include<string>
using namespace std;
int main()
{   string c;
    getline(cin,c);
    int letter,Letter,space, num, other,i;
    letter=0, Letter=0, space=0, num=0, other=0; 
    for (i=0;i<sizeof(c);i++) {
    	if (c[i]!='\0')
        if (c[i]>='a'&&c[i]<='z') letter++;
		else if (c[i]>='A'&&c[i]<='Z') Letter++; 
        else if (c[i]==' '||c[i]=='\t') space++;
        else if (c[i]>='0'&&c[i]<='9') num++;
        else other++;
    }
    cout<<"letter= "<<letter<<endl;
    cout<<"Letter= "<<Letter<<endl;
    cout<<"space= "<<space<<endl;
    cout<<"num= "<<num<<endl;
    cout<<"other= "<<other<<endl;
    return 0;
}
