#include <iostream>
#include <vector>
#include <string.h>
#include <sstream> 
using namespace std;
 
const int maxLine = 1005;			
vector<string> code[maxLine];
int col_max_len[185];				
 
int main(){
	int row = 0,col = 0;
	string line,temp;
	memset(col_max_len, 0, sizeof(col_max_len));
	while(getline(cin,line)){
		if(line[0] == '#'){			
			break;
		} 
		stringstream ss(line);	
		while(ss>>temp){
			col_max_len[col] = max(col_max_len[col],(int)temp.size());		
			code[row].push_back(temp);		
			col++;
		}
		row++; col = 0;
	} 
	for(int i = 0; i < row;i++){
		int j;
		for(j = 0; j < code[i].size() - 1; j++){
			for(int k = 0; k < code[i][j].size();k++)cout<<code[i][j][k];  
				for(int k = 0; k <= col_max_len[j] - code[i][j].size();k++)cout<<' ';	
		}
		cout<<code[i][j]<<endl;		 
	}
	return 0;
}
