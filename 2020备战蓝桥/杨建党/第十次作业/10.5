#include <stdio.h>                                

int leap(int a)                                   
{
	if(a%4==0 && a%100!=0 ||  a%400==0)
		return 1;                                 
		return 0;								
}
												
int number(int year,int month,int day)        
{
	int sum=0,int i,j;							
	int a[12]={31,28,31,30,31,30,31,31,30,31,30,31};     
	int b[12]={31,29,31,30,31,30,31,31,30,31,30,31};	

	if(leap(year)==1)							
		for(i=0;i<month-1;i++)				
			sum +=b[i];				
	else
		for(i=0;i<month-1;i++)					
			sum +=a[i];
	for(j=1990;j<year;j++)			
		if(leap(j)==1)
			sum +=366;                       
		else  
			sum +=365;					
		sum +=day;
	return sum;						
}
                                              
void main()                                      
{
	int year,month,day,n;								
	printf("请输入您想判断的年月日:");	
	scanf("%d%d%d",&year,&month,&day);				
	n=number(year,month,day);
	if((n%5)>0 && (n%5)<4)							
		printf("%d年%月%d日:打渔\n",year,month,day);
	else
		printf("%d年%d月%d日:晒网\n",year,month,day);
}

