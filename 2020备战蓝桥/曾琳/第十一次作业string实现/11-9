#include<cstdio>
#include<cstring>
char a[25];
int main()
{
	gets(a);
	for(unsigned int i=0;i<=strlen(a)-1;i++)
		if(((a[0]>='a'&&a[0]<='z')||(a[0]>='A'&&a[0]<='Z')||a[0]=='_')&&((a[i]>='0'&&a[i]<='9')||(a[i]>='a'&&a[i]<='z')||(a[i]>='A'&&a[i]<='Z')||a[i]=='_'));
			else
			{
				printf("no");
				return 0;
			}
	printf("yes");
	return 0;
 }
