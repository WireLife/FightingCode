#include<iostream>
#include<string>
using namespace std;
string s1[10];
int main()
{
	cout << "input the first string:" << endl;
	getline(cin, s1[1]);
	cout << "input the second string:" << endl;
	getline(cin, s1[2]);
	cout << "the string is:" << s1[1]<<s1[2] << endl;
	system("pause");
	return 0;
}
