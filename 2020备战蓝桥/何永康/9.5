#include <iostream>

using namespace std;

int apple(int m,int n)  //xйгф╩╧Ш  yйгелвс
{
    if(m==0||n==1)
        return 1;
    if(m<n)
        return apple(m,m);
    else
        return (apple(m,n-1)+apple(m-n,n));

}
int main()
{
    int m,n,t;
    cin>>t;
    while(t--)
    {
        cin>>m>>n;
        cout<<apple(m,n)<<endl;
    }
    return 0;
}
